{
  "name": "tdx-syntax",
  "scopeName": "source.tdx",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#string"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.tdx",
          "match": "\\b(HIGH|H|LOW|L|CLOSE|C|VOL|V)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.tdx",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.tdx",
          "match": "\\\\."
        }
      ]
    }
  }
}
